{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "core:window:default",
    "core:window:allow-start-dragging",
    "dialog:default",
    "fs:default",
    "fs:allow-open",
    "fs:allow-read",
    "fs:allow-read-file",
    "fs:allow-fstat",
    "store:default"
  ]
}
